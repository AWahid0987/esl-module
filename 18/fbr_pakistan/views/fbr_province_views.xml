<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fbr.province View list -->
    <record id="view_fbr_province_list" model="ir.ui.view">
        <field name="name">view.fbr.province.list</field>
        <field name="model">fbr.province</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View fbr.province form -->
    <record id="view_fbr_province_form" model="ir.ui.view">
        <field name="name">view.fbr.province.form</field>
        <field name="model">fbr.province</field>
        <field name="arch" type="xml">
            <form string="Fbr Province">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fbr.province search -->
    <record id="view_fbr_province_search" model="ir.ui.view">
        <field name="name">view.fbr.province.search</field>
        <field name="model">fbr.province</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fbr.province -->
    <record id="action_fbr_province" model="ir.actions.act_window">
        <field name="name">Fbr Province</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fbr.province</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fbr Province.
            </p>
        </field>
    </record>

</odoo>
