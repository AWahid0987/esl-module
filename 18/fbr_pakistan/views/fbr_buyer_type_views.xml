<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fbr.buyer.type View list -->
    <record id="view_fbr_buyer_type_list" model="ir.ui.view">
        <field name="name">view.fbr.buyer.type.list</field>
        <field name="model">fbr.buyer.type</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View fbr.buyer.type form -->
    <record id="view_fbr_buyer_type_form" model="ir.ui.view">
        <field name="name">view.fbr.buyer.type.form</field>
        <field name="model">fbr.buyer.type</field>
        <field name="arch" type="xml">
            <form string="Fbr Buyer Type">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fbr.buyer.type search -->
    <record id="view_fbr_buyer_type_search" model="ir.ui.view">
        <field name="name">view.fbr.buyer.type.search</field>
        <field name="model">fbr.buyer.type</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fbr.buyer.type -->
    <record id="action_fbr_buyer_type" model="ir.actions.act_window">
        <field name="name">Fbr Buyer Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fbr.buyer.type</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fbr Buyer Type.
            </p>
        </field>
    </record>

</odoo>
