<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fbr.sro.item.serial View list -->
    <record id="view_fbr_sro_item_serial_list" model="ir.ui.view">
        <field name="name">view.fbr.sro.item.serial.list</field>
        <field name="model">fbr.sro.item.serial</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View fbr.sro.item.serial form -->
    <record id="view_fbr_sro_item_serial_form" model="ir.ui.view">
        <field name="name">view.fbr.sro.item.serial.form</field>
        <field name="model">fbr.sro.item.serial</field>
        <field name="arch" type="xml">
            <form string="Fbr Sro Item Serial">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fbr.sro.item.serial search -->
    <record id="view_fbr_sro_item_serial_search" model="ir.ui.view">
        <field name="name">view.fbr.sro.item.serial.search</field>
        <field name="model">fbr.sro.item.serial</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fbr.sro.item.serial -->
    <record id="action_fbr_sro_item_serial" model="ir.actions.act_window">
        <field name="name">Fbr Sro Item Serial</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fbr.sro.item.serial</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fbr Sro Item Serial.
            </p>
        </field>
    </record>

</odoo>
