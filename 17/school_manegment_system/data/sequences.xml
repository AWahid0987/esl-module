<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence for Student Roll Numbers -->
        <record id="seq_school_student_roll" model="ir.sequence">
            <field name="name">Student Roll Number</field>
            <field name="code">school.student.roll</field>
            <field name="prefix">ROLL/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequence for School Salaries -->
        <record id="seq_school_salary" model="ir.sequence">
            <field name="name">School Salary Number</field>
            <field name="code">school.salary</field>
            <field name="prefix">SAL/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    <record id="seq_school_fee" model="ir.sequence">
        <field name="name">School Fee</field>
        <field name="code">school.fee</field>
        <field name="prefix">FEE/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
        <field name="implementation">no_gap</field>
    </record>

    </data>
</odoo>
