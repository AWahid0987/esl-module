<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View fbr.sale.type View Tree -->
    <record id="view_fbr_sale_type_tree" model="ir.ui.view">
        <field name="name">view.fbr.sale.type.tree</field>
        <field name="model">fbr.sale.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- View fbr.sale.type form -->
    <record id="view_fbr_sale_type_form" model="ir.ui.view">
        <field name="name">view.fbr.sale.type.form</field>
        <field name="model">fbr.sale.type</field>
        <field name="arch" type="xml">
            <form string="Fbr Sale Type">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View fbr.sale.type search -->
    <record id="view_fbr_sale_type_search" model="ir.ui.view">
        <field name="name">view.fbr.sale.type.search</field>
        <field name="model">fbr.sale.type</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action fbr.sale.type -->
    <record id="action_fbr_sale_type" model="ir.actions.act_window">
        <field name="name">Fbr Sale Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fbr.sale.type</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Fbr Sale Type.
            </p>
        </field>
    </record>

</odoo>
